/** 定义css动画 start**/

/*定义动画 上下旋转*/

.rotatejiantouOriginal {
  -webkit-transition: all 0.2s;
  transition: all 0.2s;
  transform: rotate(0deg);
}

.rotatejiantou {
  -webkit-transition: all 0.2s;
  transform: rotate(180deg);
}

/*车辆信息收缩展开动画*/

.carsInfosHeight {
  transition: height 0.2s;
}

/*定义动画 左右展开*/

.transitionWidth {
  -webkit-transition: width 0.2s linear;
  transition: width 0.2s linear;
}

.transitionAll {
  -webkit-transition: all 0.1s linear;
  transition: all 0.1s linear;
}

/**右上角工具栏 展开 收起**/

@keyframes letMove {
  from {
    width: 52px;
  }
  to {
    height: 462px;
  }
}

@keyframes rightMove {
  from {
    height: 462px;
  }
  to {
    height: 52px;
  }
}

.transitionHeight02 {
  -webkit-transition: height 0.2s;
  -moz-transition: height 0.2s;
  -o-transition: height 0.2s;
  transition: height 0.2s;
}

.hisbounce-enter-active {
  animation: bounce-in12 .5s;
}

.bounce-enter-active {
  animation: bounce-in .5s;
}

.bounce-leave-active {
  animation: bounce-in .5s reverse;
}

/** 动画 放大缩小 **/

@keyframes bounce-in {
  0% {
    transform: scale(0);
  }
  50% {
    transform: scale(1.5);
  }
  100% {
    transform: scale(1);
  }
}

/** 动画 放大缩小 **/

@keyframes bounce-in12 {
  0% {
    transform: scale(0);
  }
  50% {
    transform: scale(1.2);
  }
  100% {
    transform: scale(1);
  }
}

/** 循环动画旋转180 **/

@keyframes rotate-infinite {
  0% {
    transform: rotate(0deg);
  }
  50% {
    transform: rotate(180deg);
  }
  100% {
    transform: rotate(0deg);
  }
}

/** 循环动画  用户通讯中 闪烁 **/

@keyframes rotate-shadowColor {
  0% {
    box-shadow: 0px 0px 2px 1px rgba(82, 189, 39, 0.4);
  }
  50% {
    box-shadow: 0px 0px 4px 2px rgba(82, 189, 39, 0.8);
  }
  100% {
    box-shadow: 0px 0px 2px 1px rgba(82, 189, 39, 0.4);
  }
}

/** 上下运动 **/

@keyframes updown-infinite {
  from {
    top: -4px;
  }
  to {
    top: 3px;
  }
}

/** 旋转160deg **/

@keyframes rotate-infinite160 {
  0% {
    transform: rotate(0deg);
  }
  50% {
    transform: rotate(160deg);
  }
  100% {
    transform: rotate(0deg);
  }
}

/* 绝对定位过渡向右移动*/

.transitionParent {
  right: 35px;
  transition: right 1s;
}

.transitionRight {
  right: 0;
  transition: right 1s;
}

/* 四边小角*/

.left-top {
  position: absolute;
  width: 10px;
  height: 10px;
  border-top: 2px solid #456DA8;
  border-left: 2px solid #456DA8;
  top: 0;
  left: 0;
}

.right-top {
  position: absolute;
  width: 10px;
  height: 10px;
  border-top: 2px solid #456DA8;
  border-right: 2px solid #456DA8;
  right: 0;
  top: 0;
}

.let-bottom {
  position: absolute;
  width: 10px;
  height: 10px;
  border-left: 2px solid #456DA8;
  border-bottom: 2px solid #456DA8;
  bottom: 0;
  left: 0;
}

.right-bottom {
  position: absolute;
  width: 10px;
  height: 10px;
  border-right: 2px solid #456DA8;
  border-bottom: 2px solid #456DA8;
  bottom: 0;
  right: 0;
}
